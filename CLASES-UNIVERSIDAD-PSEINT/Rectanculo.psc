Proceso Rectanculo
	definir altura,base,arriba,lado,area,perimetro Como Real;
	Escribir "Por favor ingrese altura";
	leer altura;
	Escribir "Por favor ingrese base";
	Leer base;
	Escribir "Por favor ingrese l1";
	leer arriba;
	Escribir "Por favor ingrese l2";
	leer lado;
	Escribir "El area de el rectangulo es:", altura*base;
	Escribir "El perimetro de el rectangulo es:", altura+base+arriba+lado;
